
MTSAFE
INCLUDES = $(MONET_INCS) $(PTHREAD_INCS) 

GDK_LIBS = $(PTHREAD_LIBS) $(MALLOC_LIBS)
MONET_LIBS = $(SOCKET_LIBS) $(DL_LIBS) $(GDK_LIBS) 

STATIC_MODS = -L$(MONET_PREFIX)/lib/MonetDB \
	-l_kernel -l_arith -l_bat -l_algebra -l_sys -l_trans -l_str -l_uchr

smallTOC_SHARED_MODS =	\
	-l_mapi	\
	\
	-l_aggr	\
	-l_aggrX3	\
	-l_alarm	\
	-l_ascii_io	\
	-l_bat_arith	\
	-l_blob	\
	-l_enum	\
	-l_mkey	\
	-l_mmath	\
	-l_monettime	\
	-l_streams	\
	-l_unix	\
	-l_xtables	\
	\
	../backends/$(SQL_BACKEND)a/lib_sql_server2

EXTRA_DIST = static_modules.mx mysqldump2bulkload.py

bin_Mserver = {
	SOURCES = Mserver.mx initmodules.mx 
	LIBS = -L$(MONET_PREFIX)/lib -lmonet @SHARED_LIBS@ -lbat $(MONET_LIBS) -lmutils -lstream
}
