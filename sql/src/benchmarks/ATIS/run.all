#!/bin/sh
SQL="MapiClient -lsql"
if [ $# -eq 1 ]; then
	SQL=$1
fi

echo "loading"
$SQL < load.sql  
echo "simple join"
$SQL < Tests/select_simple_join.sql
echo "join"
$SQL < Tests/select_join.sql
echo "distinct"
$SQL < Tests/select_distinct.sql  
echo "prefix_join"
$SQL < Tests/select_key_prefix_join.sql
echo "group"
$SQL < Tests/select_group.sql 
echo "drop"
$SQL < drop.sql             
