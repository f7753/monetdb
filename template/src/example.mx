@+ Implementation Code for example module. 
@c
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>
#include <time.h>

#include "example.h"
#include "template_config.h"

int hello( str *res, str src ){
	int len = strlen(src) + strlen("hello ");
	*res = GDKmalloc(len);
	*res[0] = '\0';
	strcat(*res, "hello ");
	strcat(*res, src);
	return GDK_SUCCEED;
}
