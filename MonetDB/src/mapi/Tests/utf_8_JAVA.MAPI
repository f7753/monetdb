#!/bin/sh

if [ -f "$MONET_BUILD/src/mapi/clients/java/mapi.jar" ]; then

java -cp "$MONET_BUILD/src/mapi/clients/java/mapi.jar" MapiClient localhost $MAPIPORT << EOMIL
	print("\303\251\303\251n");
EOMIL

elif [ -f "$MONET_PREFIX/lib/MonetDB/java/mapi.jar" ]; then

java -cp "$MONET_PREFIX/lib/MonetDB/java/mapi.jar" MapiClient localhost $MAPIPORT << EOMIL
	print("\303\251\303\251n");
EOMIL

else

echo "Neither $MONET_BUILD/src/mapi/clients/java/mapi.jar nor $MONET_PREFIX/lib/MonetDB/java/mapi.jar found"

fi
