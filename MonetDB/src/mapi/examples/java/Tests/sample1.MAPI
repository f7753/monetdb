#!/bin/sh

if [ -f $MONET_BUILD/src/mapi/clients/java/mapi.jar ] ; then
	CLASSPATH=$MONET_BUILD/src/mapi/clients/java:$CLASSPATH ; export CLASSPATH
  else
	CLASSPATH=$MONET_PREFIX/lib/MonetDB/java/mapi.jar:$CLASSPATH ; export CLASSPATH
fi
if [ -f $MONET_BUILD/src/mapi/examples/java/sample1.class ] ; then
	CLASSPATH=$MONET_BUILD/src/mapi/examples/java:$CLASSPATH ; export CLASSPATH
  else
	CLASSPATH=$MONET_PREFIX/lib/MonetDB/Tests:$CLASSPATH ; export CLASSPATH
fi

Mlog -x java sample1 $MAPIPORT
