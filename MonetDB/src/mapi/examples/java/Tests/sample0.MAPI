#!/bin/sh

if [ -f $MONETDB_BUILD/src/mapi/clients/java/mapi.jar ] ; then
	CLASSPATH="`$CYGPATH_WP "$MONETDB_BUILD/src/mapi/clients/java:$CLASSPATH"`"
  else
	CLASSPATH="`$CYGPATH_WP "$MONETDB_PREFIX/share/MonetDB/lib/mapi.jar:$CLASSPATH"`"
fi
if [ -f $MONETDB_BUILD/src/mapi/examples/java/sample.class ] ; then
	CLASSPATH="`$CYGPATH_WP "$MONETDB_BUILD/src/mapi/examples/java:$CLASSPATH"`"
  else
	CLASSPATH="`$CYGPATH_WP "$MONETDB_PREFIX/share/MonetDB/Tests:$CLASSPATH"`"
fi
export CLASSPATH

Mlog -x java sample $HOST $MAPIPORT
