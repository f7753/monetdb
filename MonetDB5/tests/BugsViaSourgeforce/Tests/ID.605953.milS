# Summary:
# marking a void-T table nil
#
# marking a void-T table nil gives incorrect results. 
#
# (detected in MonetDB_TRECvid branch)

# BUG: void behaves differently from oid under mark

var t  := new(oid,str);
t.insert( oid(0), "a" );
t.insert( oid(1), "b" );
t.insert( oid(2), "c" );
t.insert( oid(3), "d" );
t.insert( oid(4), "e" );
t.insert( oid(5), "f" );
t.insert( oid(6), "g" );
t.insert( oid(7), "h" );
t.mark(nil);
t.print();

var t2  := new(void,str);
t2.seqbase(oid(0));
t2.insert( oid(0), "a" );
t2.insert( oid(1), "b" );
t2.insert( oid(2), "c" );
t2.insert( oid(3), "d" );
t2.insert( oid(4), "e" );
t2.insert( oid(5), "f" );
t2.insert( oid(6), "g" );
t2.insert( oid(7), "h" );
t2.mark(nil).print();
