	mil.line("# s:=new(int,int);\n");
	mil.line("# insert(s,5,5);\n");
	mil.line("# insert(s,3,3);\n");
	mil.line("# insert(s,4,2);\n");
	mil.line("# insert(s,2,1);\n");
	mil.line("# insert(s,1,1);\n");
	mil.line("# insert(s,4,2);\n");
	mil.line("# insert(s,4,3);\n");
	mil.line("# s.sort.print();\n");
	mil.line("# s.reverse().select(2).print();\n");
	mil.line("# ========\n");
	mil.line("#\n");
	mil.line("# incorrect output of the last select:\n");
	mil.line("# ========\n");
	mil.line("# s.reverse().select(2).print();\n");
