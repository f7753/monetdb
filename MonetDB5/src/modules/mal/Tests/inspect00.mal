# test analysis of stack frames

function tst():void;
	i:= inspect.getStackDepth();
	io.printf("stack depth %d\n",i);
	t:= inspect.getStackTrace();
	print(t);
	b0:= inspect.getStackFrame(0);
	print(b0);
	b1:= inspect.getStackFrame(1);
	print(b1);
	# next one is illegal
	b1:= inspect.getStackFrame(2);
end tst;

tst();
