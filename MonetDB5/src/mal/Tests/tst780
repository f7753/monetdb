#test debugging of a recursive function

function test(age:int,nme:str):int;
	d:= inspect.getStackDepth();
	printf("stack depth %d\n",d);
	printf("simulated error\n");
	f:= inspect.getCode();
	io.print(f);
	t:= inspect.getStackFrame();
	print(t);
	t:= inspect.getStackFrame(d);
	print(t);
end test;
test(12,"jj");
