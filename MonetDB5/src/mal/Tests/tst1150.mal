# simple stream test
	fp := streams.openWrite("/tmp/test");
	streams.writeStr(fp,"Dit is een test\n");
	streams.flush(fp);
	streams.close(fp);
	fp := streams.openRead("/tmp/test");
	s := streams.readStr(fp);
	print(s);
	streams.close(fp);

	fp := streams.openWrite("/tmp/test");
	streams.writeInt(fp,1234);
	streams.close(fp);
	fp := streams.openRead("/tmp/test");
	i := streams.readInt(fp);
	print(i);
