    io.printf("refcount =%d\n",i);	# 7 IOprint_formatted_int ($11:int)<-($6:str)(i:int)
    bat.destroy(b);	# 8 BKCdestroy ($12:void)<-(b:bat[:int,:str])
    i := bbp.getRefCount(b);	# 9 CMDgetBATrefcnt (i:int)<-(b:bat[:int,:str])
