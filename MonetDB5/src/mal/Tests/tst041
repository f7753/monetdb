# this sequence tests nil type management 
b:=nil;	print(b);

a:=23;	print(a);
a:= int(nil);	print(a);
a:= int(13);	print(a);
a:= int("23");	print(a);
a:= int(43.0:flt);	print(a);
a:= int(63.0:dbl);	print(a);

c:= true;	print(c);
c:= bit(nil);	print(c);
c:= bit(false);	print(c);
c:= bit(1);	print(c);
c:= bit("true");	print(c);
c:= bit(1.0:flt);	print(c);
c:= bit(0.0:dbl);	print(c);

d:= 3.14:flt;	print(d);
d:= flt(nil);	print(d);
d:= flt(5);	print(d);
d:= flt("7.14");	print(d);

e:= "hello";	print(e);
e:= str(nil);	print(e);
e:= str("nil");	print(e);

f:= 'a';	print(f);
f:= chr(nil);	print(f);
f:= chr("X");	print(f);
