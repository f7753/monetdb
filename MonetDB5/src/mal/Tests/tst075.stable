## parsing operations with multi-assignments
#function swap(x:str,y:str) (a:str,b:str);
#	a:=y; b:=x;
#	return (a,b);
#end swap;
function Admin.swap(x:str,y:str) (:str,b:str);	# 0  (:str)(b:str)<-(x:str)(y:str)
    a := y;	# 1  (a:str)<-(y:str)
    b := x;	# 2  (b:str)<-(x:str)
    return (a,b);	# 3  (:str)(b:str)<-(a:str)(b:str)
end swap;	# 4  
#
#(x,y):= swap("world","hello");
#print(x);
#print(y);
#
function Admin.main():int;	# 0  (main:int)
    (x,y):=Admin.swap("world","hello");	# 1  (x:str)(y:str)<-($3:str)($4:str)
    io.print(x);	# 2 CMDprint_val ($5:int)<-(x:str)
    io.print(y);	# 3 CMDprint_val ($6:int)<-(y:str)
end main;	# 4  
