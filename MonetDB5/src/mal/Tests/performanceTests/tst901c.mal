# what is the overhead of MAL in multiplex situations
profiler.setFilter("*","*");
b:= new(:lng,:lng);
t0:= alarm.usec();
i:= -1:lng;
t0:= alarm.usec();
barrier v:= nextElement(i,0:lng,1000000:lng);
	insert(b,i,i);
	redo v;
end v;
t1:= alarm.usec();
c:= copy(b);
t2:= alarm.usec();
x:= batcalc.:+=(b,b,c);
t3:= alarm.usec();
d1:= t1-t0;
d2:= t3-t2;
xc:=count(x);
printf("count %d\n",xc);
printf("make %d \n",d1);
printf("multiplex %d\n",d2);
