# Tests for the x100core package
N:= 10:lng;
res:= x100.newVector(uint,N);
col:= x100.newVector(uint,N);
sel:= x100.newVector(uint,N);

i:= -1L;
barrier v:= nextElement(i,0L,N);
	k:= rand();
	k:= k %2;
	lu:= uint(k);
	insert(res,nil,0:uint);
	insert(col,nil,lu);
	ju:= uint(i);
	insert(sel,nil,ju);
	redo v;
exit v;
print(res);
print(col);
print(sel);
printf("x100 command \n");
z:=collect(res,col,N);
printf("result %d\n",z);
print(res);
z:=collect(res,col,N);
printf("result %d\n",z);
print(res);
z:=collect(res,col,sel,N);
printf("result %d\n",z);
print(res);
