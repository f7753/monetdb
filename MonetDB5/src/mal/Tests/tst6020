# Tests for the x100core package
N:= 10:lng;
res:= x100.newVector(uint,N);
col:= x100.newVector(uint,N);
sel:= x100.newVector(uint,N);

i:= -1L;
barrier v:= nextElement(i,0L,N);
	insert(res,nil,0:uint);
	ju:= uint(i);
	insert(col,nil,ju);
	insert(sel,nil,ju);
	redo v;
exit v;
print(res);
print(col);
#print(sel);
printf("x100 command \n");
cst:= uint(5);
z:= x100.+(res,col,col,sel,N);
printf("answer %d\n",z);
print(res);
z:= x100.-(res,col,N);
printf("answer %d\n",z);
print(res);
