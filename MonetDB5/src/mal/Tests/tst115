# the next step is to trace the call of a mal function call

command + (L:any::1,R:any::1):any::1 = addIntInt; 

function doitOnce():int;
	z:= 12;
	return z;
end doitOnce;

function doitTwice(i:int):int;
	j:= 2;
	k:= 11111;
	i:= j+k;
	return i;
end doitTwice;


a:=1;
b:= doitOnce();
c:= 2;
d:= doitTwice(3);
c:= 3;
d:= doitTwice("wrong");
