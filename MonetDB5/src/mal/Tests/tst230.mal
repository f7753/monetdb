# a test checking BAT reference counting under various situations
	b:= new(:int,:str);
	i:= getRefCount(b);
	printf("refcount =%d\n",i);

	insert(b,1,"bat B");
	print(b);
	i:= getRefCount(b);
	printf("second refcount =%d\n",i);

	# referenceing through mirroring
	b5:= reverse(b);
	i:= getRefCount(b);
	printf("after reverse assignment refcount =%d\n",i);

