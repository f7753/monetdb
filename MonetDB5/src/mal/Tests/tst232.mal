# a test checking BAT reference counting under various situations
	bbp.open();
	b:= new(:int,:str);
	i:= getRefCount(b);
	printf("refcount =%d\n",i);

	insert(b,1,"bat B");
	print(b);
	i:= getRefCount(b);
	printf("refcount =%d\n",i);

	bbp.destroy(b);
	i:= getRefCount(b);
	printf("after destroy refcount =%d\n",i);
